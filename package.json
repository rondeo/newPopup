{
  "name": "chat",
  "version": "1.0.0",
  "description": "chat",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --open --mode development",
    "build": "webpack --mode production --watch",
    "test": "mocha --opts ./config/mocha.opts src/**/*.test.js --recursive -R nyan --silent",
    "test-cov": "nyc -e .jsx mocha --opts ./config/mocha.opts src/**/*.test.js --recursive -R nyan --silent",
    "update-snapshots": "mocha --opts ./config/mocha.opts src/**/*.test.js --require mocha-snapshots --update"
  },
  "author": "Alex",
  "license": "ISC",
  "nyc": {
    "check-coverage": false,
    "per-file": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "require": [
      "@babel/register"
    ],
    "exclude": [
      "coverage/**",
      "test/**",
      "test{,-*}.js",
      "**/*.test.js",
      "**/__tests__/**",
      "**/node_modules/**",
      "**/__snapshots__/**",
      ".nyc_output/**",
      ".nyc_output-tmp/**",
      "config/mocha-setup.js",
      "config/mocha.opts",
      "**/mockData/**"
    ],
    "cache": true,
    "all": false,
    "temp-directory": "./.nyc_output-tmp",
    "report-dir": "./.nyc_output"
  },
  "dependencies": {
    "@material-ui/core": "^4.1.1",
    "@material-ui/icons": "^4.2.0",
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^3.0.6",
    "chance": "^1.0.18",
    "core-js": "^3.1.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "nodemon": "^1.19.0",
    "rc-queue-anim": "^1.6.12",
    "react": "^16.8.6",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-scripts": "2.1.8",
    "react-svg-loader": "^3.0.2",
    "react-swipeable-views": "^0.13.3",
    "react-transition-group": "^1.2.1",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.2",
    "simplebar-react": "^1.0.0-alpha.1",
    "uuid": "^3.3.2",
    "ws": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/polyfill": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "@svgr/cli": "^4.3.0",
    "@svgr/webpack": "^4.3.0",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^2.0.2",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "html-webpack-plugin": "^3.2.0",
    "ignore-styles": "^5.0.1",
    "install": "^0.12.2",
    "jsdom": "^14.0.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mocha": "^6.0.2",
    "mocha-snapshots": "^4.2.0",
    "npm": "^6.9.0",
    "null-loader": "^0.1.1",
    "nyc": "^13.3.0",
    "prop-types": "^15.7.2",
    "react-scrollbars-custom": "^4.0.0-alpha.6",
    "sinon": "^7.3.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-clean-plugin": "^0.2.3",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  }
}
